# Render Worker Environment Variables

# Worker Configuration
WORKER_CONCURRENCY=2
NODE_ENV=development

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/terrashaper

# Redis (for BullMQ)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# AI Providers
GOOGLE_IMAGEN_API_KEY=your-google-imagen-key
GOOGLE_IMAGEN_ENDPOINT=https://us-central1-aiplatform.googleapis.com
OPENAI_API_KEY=your-openai-key

# Google Cloud Storage
GCS_BUCKET_NAME=terrashaper-renders
GCS_PROJECT_ID=your-project-id
GCS_KEY_FILE_PATH=../../gcs-credentials.json

# Logging
LOG_LEVEL=info