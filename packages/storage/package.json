{
  "name": "@terrashaper/storage",
  "version": "1.0.0",
  "description": "Google Cloud Storage integration for TerraShaperPro",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "check-types": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.7.0",
    "@svgr/webpack": "^8.1.0",
    "@trpc/server": "^11.4.2",
    "clsx": "^2.1.1",
    "mime-types": "^2.1.35",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.0.0",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.7",
    "eslint": "^8.57.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "tsup": "^8.0.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.8"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ]
}
